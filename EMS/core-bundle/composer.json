{
    "name": "elasticms/core-bundle",
    "description": "The elasticms core bundle",
    "type": "symfony-bundle",
    "keywords": [
        "elasticms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "EMS Community",
            "homepage": "https://github.com/ems-project/elasticms/contributors"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": "^8.4",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "caxy/php-htmldiff": "^0.1",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "dragonmantank/cron-expression": "^3.3",
        "elasticms/submission-bundle": "6.0.*",
        "elasticms/xliff": "6.0.*",
        "guzzlehttp/guzzle": "^7.8",
        "maennchen/zipstream-php": "^3.0",
        "scssphp/scssphp": "^2.0",
        "sensiolabs/ansi-to-html": "^1.2",
        "symfony/asset": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/form": "6.4.*",
        "symfony/ldap": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/monolog-bridge": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-twig-component": "^2.13",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*"
    },
    "autoload": {
        "psr-4": {
            "EMS\\CoreBundle\\": "src/"
        }
    },
    "config": {
        "lock": false,
        "sort-packages": true
    }
}
