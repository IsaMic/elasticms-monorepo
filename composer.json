{
    "name": "ems-project/elasticms",
    "description": "ElasticMS's monorepo",
    "authors": [
        {
            "name": "EMS Community",
            "homepage": "https://github.com/ems-project/elasticms/contributors"
        }
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-soap": "*",
        "ext-sqlite3": "*",
        "ext-tidy": "*",
        "ext-zip": "*",
        "aws/aws-sdk-php": "^3.294",
        "caxy/php-htmldiff": "^0.1",
        "cebe/markdown": "^1.2",
        "composer/ca-bundle": "^1.4",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.3",
        "dompdf/dompdf": "^v3.1",
        "dragonmantank/cron-expression": "^3.3",
        "endroid/qr-code-bundle": "^6.0",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.8",
        "kevinrob/guzzle-cache-middleware": "^6.0",
        "league/flysystem-memory": "^3.19",
        "league/flysystem-sftp-v3": "^3.22",
        "maennchen/zipstream-php": "^3.0",
        "onelogin/php-saml": "^4.1",
        "phpoffice/phpspreadsheet": "^3.8",
        "promphp/prometheus_client_php": "^2.9",
        "psr/simple-cache": "^3.0",
        "ramsey/uuid-doctrine": "^2.1",
        "ruflin/elastica": "^7.3",
        "scssphp/scssphp": "^2.0",
        "sensiolabs/ansi-to-html": "^1.2",
        "stevenmaguire/oauth2-keycloak": "^5.1",
        "symfony-cmf/routing": "^3.0",
        "symfony/asset": "6.4.*",
        "symfony/cache": "6.4.*",
        "symfony/config": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dependency-injection": "6.4.*",
        "symfony/dom-crawler": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/error-handler": "6.4.*",
        "symfony/event-dispatcher": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/flex": "^2.4",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/html-sanitizer": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/ldap": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bridge": "6.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/options-resolver": "6.4.*",
        "symfony/password-hasher": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/routing": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/security-core": "6.4.*",
        "symfony/security-csrf": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bridge": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-twig-component": "^2.13",
        "symfony/validator": "6.4.*",
        "symfony/var-exporter": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/yaml": "6.4.*",
        "thenetworg/oauth2-azure": "^2.2",
        "twig/cache-extra": "^3.8",
        "twig/cssinliner-extra": "^3.8",
        "twig/extra-bundle": "^3.8",
        "twig/html-extra": "^3.8",
        "twig/inky-extra": "^3.8",
        "twig/intl-extra": "^3.8",
        "twig/markdown-extra": "^3.8",
        "twig/string-extra": "^3.8",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.41",
        "http-interop/http-factory-guzzle": "^1.2",
        "knplabs/github-api": "^3.13",
        "knplabs/packagist-api": "^2.1",
        "mockery/mockery": "^1.6",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "symplify/monorepo-builder": "^11.2"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": false,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\Admin\\": "elasticms-admin/src/",
            "App\\CLI\\": "elasticms-cli/src/",
            "App\\Web\\": "elasticms-web/src/",
            "EMS\\AdminUIBundle\\": "EMS/admin-ui-bundle/src/",
            "EMS\\ClientHelperBundle\\": "EMS/client-helper-bundle/src/",
            "EMS\\CommonBundle\\": "EMS/common-bundle/src/",
            "EMS\\CoreBundle\\": "EMS/core-bundle/src/",
            "EMS\\FormBundle\\": "EMS/form-bundle/src/",
            "EMS\\Helpers\\": "EMS/helpers/src/",
            "EMS\\SubmissionBundle\\": "EMS/submission-bundle/src/",
            "EMS\\Xliff\\": "EMS/xliff/src/",
            "Application\\Migrations\\Scripts\\": "EMS/core-bundle/migrations/scripts"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Admin\\Tests\\": "elasticms-admin/tests/",
            "App\\CLI\\Tests\\": "elasticms-cli/tests/",
            "App\\Web\\Tests\\": "elasticms-admin/tests/",
            "EMS\\ClientHelperBundle\\Tests\\": "EMS/client-helper-bundle/tests/",
            "EMS\\CommonBundle\\Tests\\": "EMS/common-bundle/tests/",
            "EMS\\CoreBundle\\Tests\\": "EMS/core-bundle/tests/",
            "EMS\\FormBundle\\Tests\\": "EMS/form-bundle/tests/",
            "EMS\\Helpers\\Tests\\": "EMS/helpers/tests/",
            "EMS\\SubmissionBundle\\Tests\\": "EMS/submission-bundle/tests/",
            "EMS\\Xliff\\Tests\\": "EMS/xliff/tests/",
            "Build\\": "build/src/"
        }
    },
    "replace": {
        "elasticms/admin-ui-bundle": "self.version",
        "elasticms/client-helper-bundle": "self.version",
        "elasticms/common-bundle": "self.version",
        "elasticms/core-bundle": "self.version",
        "elasticms/form-bundle": "self.version",
        "elasticms/helpers": "self.version",
        "elasticms/submission-bundle": "self.version",
        "elasticms/xliff": "self.version",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "rector": "rector --config=./config/rector.php",
        "monorepo-validate": "monorepo-builder --config=./config/monorepo-builder.php validate",
        "phpcs": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer --config=./config/php-cs-fixer.php fix",
        "phpstan": "phpstan --configuration=./config/phpstan.neon.dist --memory-limit=1G analyse",
        "phpstan-baseline": "phpstan --configuration=./config/phpstan.neon.dist --memory-limit=1G --generate-baseline=./config/phpstan-baseline.neon analyse",
        "phpunit": "phpunit -c config/phpunit.xml.dist --display-deprecations",
        "phpunit-admin": "phpunit -c elasticms-admin/phpunit.xml.dist --display-deprecations",
        "phpunit-cli": "phpunit -c elasticms-cli/phpunit.xml.dist --display-deprecations",
        "phpunit-web": "phpunit -c elasticms-web/phpunit.xml.dist --display-deprecations",
        "phpunit-all": [
            "@phpunit",
            "@phpunit-admin",
            "@phpunit-cli",
            "@phpunit-web"
        ],
        "phpall": [
            "@phpunit",
            "@phpstan",
            "@phpcs"
        ],
        "lint": [
            "@lint-container",
            "@lint-twig"
        ],
        "lint-container": [
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:container",
            "APP_DEBUG=1 php elasticms-cli/bin/console lint:container",
            "APP_DEBUG=1 php elasticms-web/bin/console lint:container"
        ],
        "lint-twig": [
            "@lint-twig-bundles",
            "@lint-twig-demo"
        ],
        "lint-twig-bundles": [
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSAdminUIBundle --show-deprecations",
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSClientHelperBundle --show-deprecations",
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSCommonBundle --show-deprecations",
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSCoreBundle --show-deprecations",
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSFormBundle --show-deprecations",
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig @EMSSubmissionBundle --show-deprecations"
        ],
        "lint-twig-demo": [
            "APP_DEBUG=1 php elasticms-admin/bin/console lint:twig demo/skeleton/template_ems --show-deprecations",
            "APP_DEBUG=1 php elasticms-web/bin/console lint:twig demo/skeleton/template --show-deprecations"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
